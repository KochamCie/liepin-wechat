.d-user-login{
    position: relative;
    margin: 0px 32px;
    height: 3rem;
    width: 350px;
}
.d-user-login .icon-account{
    width: 3rem;
    height: 3rem;
    position: absolute;
    top: -3.1rem;
    left: 30px;
    
}
.d-user-login .icon-password{
    /* background: url(password32.png) no-repeat #899dda center; */
    width: 3rem;
    height: 3rem;
    position: absolute;
    top: -3.1rem;
    left: 30px;
}
form .input {
    padding-left: 3rem;
    height: 3rem;
    width: 70%;
    background-color: #fff;
    margin: 0 auto;
}

.btn {
    /* display: inline-block; */
    padding: 6px 12px;
    margin-bottom: 0;
    /* font-size: 14px; */
    font-weight: 400;
    line-height: 1.428571429;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    cursor: pointer;
    border: 1px solid transparent;
    border-radius: 4px;
    background-color: #1a8fb4;
    color: white;
    margin: 31px;
}

